oot 2.5.x系列",collapsable:!1,sidebarDepth:0,children:["springboot/springboot","springboot/springboot-x-overview","springboot/springboot-x-hello-world","springboot/springboot-x-hello-world-mvc","springboot/springboot-x-hello-h2-jpa","springboot/springboot-x-hello-banner","springboot/springboot-x-hello-logback","springboot/springboot-x-hello-devtool","springboot/springboot-x-hello-anno","springboot/springboot-x-interface-response","springboot/springboot-x-interface-param","springboot/springboot-x-interface-param-i18n","springboot/springboot-x-interface-exception","springboot/springboot-x-interface-version","springboot/springboot-x-interface-doc","springboot/springboot-x-interface-doc-smart","springboot/springboot-x-interface-doc-tool","springboot/springboot-x-interface-3rd","springboot/springboot-x-interface-mideng","springboot/springboot-x-interface-jiami","springboot/springboot-x-interface-xianliu","springboot/springboot-x-interface-xianliu-dist","springboot/springboot-x-mysql-jpa","springboot/springboot-x-mysql-mybatis-xml","springboot/springboot-x-mysql-mybatis-anno","springboot/springboot-x-mysql-mybatis-page","springboot/springboot-x-mysql-mybatis-multi-ds","springboot/springboot-x-mysql-mybatis-plus","springboot/springboot-x-mysql-mybatis-plus-gen","springboot/springboot-x-mysql-mybatis-plus-multi-tenant","springboot/springboot-x-mysql-shardingjdbc","springboot/springboot-x-mysql-HikariCP","springboot/springboot-x-mysql-druid","springboot/springboot-x-mysql-liquibase","springboot/springboot-x-mysql-flyway","springboot/springboot-x-redis-jedis","springboot/springboot-x-postgre-jpa","springboot/springboot-x-postgre-jpa-jsonb","springboot/springboot-x-mongodb-template","springboot/springboot-x-elastic-template","springboot/springboot-x-socket-websocket","springboot/springboot-x-socket-netty","springboot/springboot-x-task-timer","springboot/springboot-x-task-executor-timer","springboot/springboot-x-task-spring-task-timer","springboot/springboot-x-task-hashwheeltimer